<menubar>
  <menu text="Main" mnemonic="VK_M" accelerator="control m">
    <menuitem text="Export" actionlistener="rootFrameExportMenuActionListener" mnemonic="VK_E" accelerator="control e" />
    <menuitem text="Import" actionlistener="rootFrameImportMenuActionListener" mnemonic="VK_I" accelerator="control i" />
    <menusplit />
    <menuitem text="Exit" actionlistener="rootFrameExitActionListener" mnemonic="VK_X" />
  </menu>
  <menu text="Help" mnemonic="VK_H" accelerator="control h">
    <menuitem text="About" actionlistener="rootFrameAboutMenuActionListener" mnemonic="VK_A" accelerator="control a" />
  </menu>
</menubar>
